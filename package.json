{
	"name": "opencv-react-face-detection",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"preview": "vite preview",
		"format": "biome format --write . && prettier . --write",
		"lint": "biome lint . && prettier . --check",
		"check": "biome check --write . && prettier . --write",
		"tfrm": "cd deploy && node rmtf.mjs",
		"tfinit": "cd deploy && terraform init -upgrade",
		"tfapply": "cd deploy && terraform apply -auto-approve",
		"deploy": "bun run build && cd deploy && terraform apply -auto-approve",
		"destroy": "cd deploy && terraform destroy -auto-approve"
	},
	"dependencies": {
		"@techstark/opencv-js": "^4.10.0-release.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.26.2",
		"react-webcam": "^7.2.0"
	},
	"devDependencies": {
		"@types/react": "^18.3.8",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react-swc": "^3.7.0",
		"globals": "^15.9.0",
		"typescript": "^5.6.2",
		"vite": "^5.4.7"
	}
}
